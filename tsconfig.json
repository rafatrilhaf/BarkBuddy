{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "baseUrl": ".",
    "strict": true,
    "paths": {"@/*": ["./*"]},
     "moduleResolution": "Bundler",
    "moduleSuffixes": [".native", ".ios", ".android", ".web", ""]
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts"
  ]
}
